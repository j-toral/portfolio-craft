{
  "require": {
    "born05/craft-assetusage": "3.3.0",
    "craftcms/ckeditor": "3.7.1",
    "craftcms/cms": "4.7.1",
    "marionnewlevant/twig-perversion": "^3.0",
    "mmikkel/retcon": "2.7.5",
    "vaersaagod/geomate": "^2.1",
    "verbb/field-manager": "3.0.8",
    "verbb/navigation": "2.0.25",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}